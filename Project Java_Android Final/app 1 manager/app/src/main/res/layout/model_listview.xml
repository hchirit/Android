<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/textView36"
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:background="#10ffeb"
            android:text="Model"
            android:textAlignment="center"
            android:textSize="12sp" />

        <TextView
            android:background="#10ffeb"
            android:id="@+id/textView35"
            android:layout_width="98dp"
            android:layout_height="wrap_content"
            android:text="manufacture"
            android:textAlignment="center"
            android:textSize="12sp" />

        <TextView
            android:background="#10ffeb"
            android:id="@+id/textView34"
            android:layout_width="97dp"
            android:layout_height="wrap_content"
            android:text="Name"
            android:textAlignment="center"
            android:textSize="12sp" />

        <TextView
            android:background="#10ffeb"
            android:id="@+id/textView33"
            android:layout_width="96dp"
            android:layout_height="wrap_content"
            android:text="EngineCapacity"
            android:textAlignment="center"
            android:textSize="12sp" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/id_model_"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/id_maufacturer_"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/id_name_model_"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/id_enginecapacity_"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="TextView"
            android:textAlignment="center" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:background="#10ffeb"
            android:id="@+id/textView32"
            android:layout_width="94dp"
            android:layout_height="wrap_content"
            android:text="PlaceNumber"
            android:textAlignment="center"
            android:textSize="12sp" />

        <TextView
            android:background="#10ffeb"
            android:id="@+id/textView31"
            android:layout_width="88dp"
            android:layout_height="wrap_content"
            android:text="DoorNumber"
            android:textAlignment="center"
            android:textSize="12sp" />

        <TextView
            android:background="#10ffeb"
            android:id="@+id/textView37"
            android:layout_width="41dp"
            android:layout_height="wrap_content"
            android:text="Gps"
            android:textAlignment="center"
            android:textSize="12sp" />

        <TextView
            android:background="#10ffeb"
            android:id="@+id/textView38"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Luggage"
            android:textAlignment="center"
            android:textSize="12sp" />

        <TextView
            android:background="#10ffeb"
            android:id="@+id/textView30"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="Airconditionner"
            android:textAlignment="center"
            android:textSize="12sp" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/id_placenumber_"
            android:layout_width="95dp"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/id_doornumber_"
            android:layout_width="86dp"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/id_check_gps_"
            android:layout_width="42dp"
            android:layout_height="wrap_content"
            android:text="Te"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/id_check_luggage_"
            android:layout_width="68dp"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textAlignment="center" />

        <TextView
            android:id="@+id/id_check_air_"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:textAlignment="center" />

    </LinearLayout>

    <TextView
        android:id="@+id/textView29"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textSize="4sp"
        android:background="#000000"/>


</LinearLayout>