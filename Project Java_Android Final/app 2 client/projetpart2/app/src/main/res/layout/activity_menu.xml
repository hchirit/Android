<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView15"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="27dp"
            android:text="Main Menu"
            android:textAllCaps="false"
            android:textColor="#000000"
            android:textSize="55sp"
            android:textStyle="bold"
            android:layout_below="@+id/linearLayout10"
            android:layout_alignRight="@+id/button15"
            android:layout_alignEnd="@+id/button15" />

        <Button
            android:id="@+id/button15"
            android:layout_width="276dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="41dp"
            android:background="#544e4e"
            android:text="Display Cars Per Branch"
            android:textAllCaps="false"
            android:textColor="#ffffff"
            android:textSize="30sp"
            android:layout_below="@+id/textView15"
            android:layout_centerHorizontal="true" />

        <Button
            android:id="@+id/button17"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/button15"
            android:layout_alignLeft="@+id/button15"
            android:layout_alignRight="@+id/button15"
            android:layout_alignStart="@+id/button15"
            android:layout_below="@+id/button15"
            android:layout_marginTop="38dp"
            android:background="#544e4e"
            android:text="Display Branch Per Model"
            android:textAllCaps="false"
            android:textColor="#ffffff"
            android:textSize="30sp" />

        <Button
            android:id="@+id/button4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignEnd="@+id/button17"
            android:layout_alignLeft="@+id/button17"
            android:layout_alignRight="@+id/button17"
            android:layout_alignStart="@+id/button17"
            android:layout_below="@+id/button17"
            android:layout_marginTop="30dp"
            android:background="#544e4e"
            android:text="Check My Reservation"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textColor="#ffffff"
            android:textSize="30sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_weight="1"
            android:orientation="horizontal"
            android:weightSum="1"
            android:id="@+id/linearLayout10">

            <android.support.design.widget.AppBarLayout
                android:id="@+id/appBarLayout_model"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:theme="@style/AppTheme.AppBarOverlay"
                android:weightSum="1">>
                <Button
                    android:id="@+id/button_return_1"
                    android:layout_width="64dp"
                    android:layout_height="47dp"
                    android:layout_weight="1"
                    android:drawableBottom="@drawable/ic_arrow_back_black_24dp" />
            </android.support.design.widget.AppBarLayout>

        </LinearLayout>
     </RelativeLayout>


</RelativeLayout>